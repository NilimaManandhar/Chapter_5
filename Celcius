/**
 *
 * @author Nilima M
 */
public class CtoF {
    public static void main(String[] args)
    {
        double f;
        for(int i=0; i<=20;i++)
        {
            f=((9/5)*i)+32;
            System.out.println("For "+i + " degree c = "+ f+ "fahrenheit");
        }
    }
}
